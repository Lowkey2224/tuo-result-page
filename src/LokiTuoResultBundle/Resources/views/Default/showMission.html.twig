{% extends '::base.html.twig' %}

{% block body %}
    Result for {{ mission.name }}<br/>
    {% for result in results %}
        Player: {{ result.player.name }} with {{ result.percent/10 | number_format(2)}}%<br/>
        {% for deckEntry in result.deck %}
            {{ deckEntry.card.name }}<br/>
        {% endfor %}
        <hr>
    {% endfor %}
{% endblock %}

